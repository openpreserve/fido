    <signature>
      <name>Microsoft Visio Drawing (5.0) - FIDO patch</name>
      <pattern>
        <position>BOF</position>
        <regex>(?s)Visio \(TM\) Drawing\x0d\x0a</regex>
      </pattern>
    </signature>
  </format>
